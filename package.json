{
  "name": "infinity-tc-launcher",
  "version": "0.0.6",
  "description": "SWG Infinity TC Launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "build --win --ia32 -p never",
    "deploy": "build --win --ia32 -p always"
  },
  "repository": "https://github.com/swginfinity/Inf-Launcher-TC",
  "homepage": "https://www.swginfinity.com",
  "keywords": [
    "swg infinity",
    "launcher",
    "installer",
    "swg",
    "star",
    "wars",
    "galaxies",
    "emulator",
    "emu",
    "swgemu"
  ],
  "author": "Progor adapted by Qrave and MrObvious",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^11.2.3",
    "electron-builder": "^20.44.4"
  },
  "dependencies": {
    "adm-zip": "^0.5.2",
    "electron-is-dev": "^0.2.0",
    "electron-log": "^4.3.1",
    "electron-updater": "^4.3.5",
    "follow-redirects": "^1.13.2",
    "mkdirp": "^1.0.4",
    "node": "^15.8.0",
    "npm": "^7.5.4",
    "random-access-file": "^2.1.5",
    "request": "^2.88.2"
  },
  "build": {
    "productName": "Infinity TC Launcher",
    "win": {
      "requestedExecutionLevel": "highestAvailable",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "perMachine": true
    }
  }
}
